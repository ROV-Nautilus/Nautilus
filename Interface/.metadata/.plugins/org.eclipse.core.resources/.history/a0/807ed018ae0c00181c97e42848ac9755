package Capteurs;

import SSH.Exec;

public class Pression  implements Runnable{

	public int Pression = 10;
	public int TemperatureC = 20;
	public int TemperatureF = 30;
	
	public Pression() {
		
	}
	
	public void run() {
		Exec ex3 = new Exec("python MS5803_14BA.py");
		Thread a3 = new Thread(ex3);
		a3.start();
		while( a3.isAlive()) {
		}
		System.out.println(ex3.retour);
		String[] a = ex3.retour.split("/");
		this.Pression = Integer.valueOf(a[0]);
		this.TemperatureC = Integer.valueOf(a[1]);
		this.TemperatureF = Integer.valueOf(a[2]);
		System.out.println(" "+Pression);
	}

}
